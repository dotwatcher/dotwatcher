{
	"version": 2,
	"alias": ["www.dotwatcher.cc", "dotwatcher.cc"],
	"env": {
		"CONTENTFUL_ACCESS_TOKEN": "@contentful-api-token",
		"PUSHER_APP_ID": "@pusher-app-id",
		"PUSHER_KEY": "@pusher-key",
		"PUSHER_SECRET": "@pusher-secret",
		"MAILCHIMP": "@mailchimp",
		"ANALYTICS": "@analytics",
		"SCRAPEY_API_KEY": "@scrapey-api-key",
		"PGHOST": "@pghost",
		"PGUSER": "@pguser",
		"PGPASSWORD": "@pgpassword",
		"PGDATABASE": "@pgdatabase",
		"PGPORT": "@pgport"
	},
	"build": {
		"env": {
			"CONTENTFUL_ACCESS_TOKEN": "@contentful-api-token",
			"PUSHER_APP_ID": "@pusher-app-id",
			"PUSHER_KEY": "@pusher-key",
			"PUSHER_SECRET": "@pusher-secret",
			"MAILCHIMP": "@mailchimp",
			"ANALYTICS": "@analytics",
			"SCRAPEY_API_KEY": "@scrapey-api-key",
			"BASEURL": "@dotwatcher-url"
		}
	},
	"builds": [
		{
			"src": "next.config.js",
			"use": "@now/next"
		}
	],
	"routes": [
		{
			"src": "/page/(?<id>[^/]+)$",
			"dest": "/page?id=$id"
		},
		{
			"src": "/post/(?<id>[^/]+)$",
			"dest": "/post?id=$id"
		},
		{
			"src": "/race/(?<slug>[^/]+)$",
			"dest": "/race?slug=$slug"
		},
		{
			"src": "/feature/(?<slug>[^/]+)$",
			"dest": "/feature?slug=$slug"
		},
		{
			"src": "/results/(?<year>[^/]+)?/(?<race>[^/]+)$",
			"dest": "/results?year=$year&race=$race"
		},
		{
			"src": "/profile/(?<name>[^/]+)$",
			"dest": "/profile?name=$name"
		},
		{
			"src": "/calander",
			"dest": "/profile"
		}
	]
}
