{
	"version": 2,
	"alias": ["www.dotwatcher.cc", "dotwatcher.cc"],
	"env": {
		"CONTENTFUL_ACCESS_TOKEN": "@contentful-api-token",
		"PUSHER_APP_ID": "@pusher-app-id",
		"PUSHER_KEY": "@pusher-key",
		"PUSHER_SECRET": "@pusher-secret",
		"MAILCHIMP": "@mailchimp",
		"ANALYTICS": "@analytics",
		"SCRAPEY_API_KEY": "@scrapey-api-key",
		"PGHOST": "@pghost",
		"PGUSER": "@pguser",
		"PGPASSWORD": "@pgpassword",
		"PGDATABASE": "@pgdatabase",
		"PGPORT": "@pgport",
		"CLOUDINARY_NAME": "@cloudinary-name",
		"CLOUDINARY_PRESET": "@cloudinary-preset",
		"CLOUDINARY_KEY": "@cloudinary-key",
		"CLOUDINARY_SECRET": "@cloudinary-secret",
		"AUTH0_DOMAIN": "@auth0-domain",
		"AUTH0_CLIENT_ID": "@auth0-client-id",
		"AUTH0_CLIENT_SECRET": "@auth0-client-secret",
		"AUTH0_APP_CLIENT_ID": "@auth0-app-client-id",
		"AUTH0_APP_CLIENT_SECRET": "@auth0-app-client-secret",
		"AUTH0_APP_COOKIE_SECRET": "@auth0-app-cookie-secret",
		"AUTH0_CALLBACK_DOMAIN": "@auth0-callback-domain"
	},
	"build": {
		"env": {
			"CONTENTFUL_ACCESS_TOKEN": "@contentful-api-token",
			"PUSHER_APP_ID": "@pusher-app-id",
			"PUSHER_KEY": "@pusher-key",
			"PUSHER_SECRET": "@pusher-secret",
			"MAILCHIMP": "@mailchimp",
			"ANALYTICS": "@analytics",
			"SCRAPEY_API_KEY": "@scrapey-api-key",
			"BASEURL": "@dotwatcher-url",
			"CLOUDINARY_NAME": "@cloudinary-name",
			"CLOUDINARY_PRESET": "@cloudinary-preset",
			"CLOUDINARY_KEY": "@cloudinary-key",
			"CLOUDINARY_SECRET": "@cloudinary-secret",
			"AUTH0_DOMAIN": "@auth0-domain",
			"AUTH0_CLIENT_ID": "@auth0-client-id",
			"AUTH0_CLIENT_SECRET": "@auth0-client-secret",
			"AUTH0_APP_CLIENT_ID": "@auth0-app-client-id",
			"AUTH0_APP_CLIENT_SECRET": "@auth0-app-client-secret",
			"AUTH0_APP_COOKIE_SECRET": "@auth0-app-cookie-secret",
			"AUTH0_CALLBACK_DOMAIN": "@auth0-callback-domain"
		}
	},
	"builds": [
		{
			"src": "next.config.js",
			"use": "@now/next"
		}
	],
	"routes": [
		{
			"src": "/profile/Zoe%20Chauderlot",
			"dest": "/profile/Zoé%20Chauderlot",
			"status": 301
		},
		{
			"src": "/profile/Bjorn%20Lenhard",
			"dest": "/profile/Björn%20Lenhard",
			"status": 301
		},
		{
			"src": "/profile/Zoe%20Chauderlot",
			"dest": "/profile/Zoé%20Chauderlot",
			"status": 301
		},
		{
			"src": "/profile/Stuart%20Birnie",
			"dest": "profile/Stuart%20Birnie%20(Hippy)",
			"status": 301
		},
		{
			"src": "/page/(?<id>[^/]+)$",
			"dest": "/page?id=$id"
		},
		{
			"src": "/post/(?<id>[^/]+)$",
			"dest": "/post?id=$id"
		},
		{
			"src": "/race/(?<slug>[^/]+)$",
			"dest": "/race?slug=$slug"
		},
		{
			"src": "/feature/(?<slug>[^/]+)$",
			"dest": "/feature?slug=$slug"
		},
		{
			"src": "/results/(?<year>[^/]+)?/(?<race>[^/]+)$",
			"dest": "/results?year=$year&race=$race"
		},
		{
			"src": "/profile/edit",
			"dest": "/profile/edit"
		},
		{
			"src": "/profile/(?<name>[^/]+)$",
			"dest": "/profile?name=$name"
		},
		{
			"src": "/calendar/(?<year>[^/]+)?/(?<month>[^/]+)$",
			"dest": "/calendar?year=$year&month=$month"
		}
	]
}
